								"exec": [
									"pm.test(\"Response status code is 201\", function () {",
									"    pm.expect(pm.response.code).to.eql(201);",
									"});",
									"",
									"",
									"pm.test(\"The 'id' must be a non-empty string\", function () {",
									"    const responseData = pm.response.json();",
									"    ",
									"    pm.expect(responseData).to.have.property('id').that.is.a('string').and.is.not.empty;",
									"});",
									"",
									"",
									"pm.test(\"Price must be a non-negative number\", function () {",
									"    const responseData = pm.response.json();",
									"    ",
									"    pm.expect(responseData).to.be.an('object');",
									"    pm.expect(responseData.price).to.be.a('number').and.to.be.at.least(0, \"Price should not be negative\");",
									"});",
									"",
									"",
									"pm.test(\"The tags array must contain at least one non-empty string\", function () {",
									"    const responseData = pm.response.json();",
									"    ",
									"    pm.expect(responseData).to.be.an('object');",
									"    pm.expect(responseData.tags).to.be.an('array').that.has.lengthOf.at.least(1, \"Array should not be empty\");",
									"    ",
									"    responseData.tags.forEach(tag => {",
									"        pm.expect(tag).to.be.a('string').that.is.not.empty;",
									"    });",
									"});",
									"",
									"",
									"pm.test(\"Ratings array contains valid objects\", function () {",
									"    const responseData = pm.response.json();",
									"    ",
									"    pm.expect(responseData).to.be.an('object');",
									"    pm.expect(responseData.ratings).to.be.an('array').that.is.not.empty;",
									"",
									"    responseData.ratings.forEach(rating => {",
									"        pm.expect(rating).to.be.an('object');",
									"        pm.expect(rating.score).to.be.a('number').that.is.at.least(0);",
									"        pm.expect(rating.comment).to.be.a('string').that.is.not.empty;",
									"    });",
									"});"
								],
